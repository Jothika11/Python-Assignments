        },
        "outputId": "38bccd85-672d-4566-b476-9e2256151631"
      },
      "source": [
        "print('Prime Numbers:')\n",
        "for i in range(2,201):\n",
        "    for j in range(2,i):\n",
        "        if i%j==0:\n",
        "            break\n",
        "    else:\n",
        "        print(i)"
